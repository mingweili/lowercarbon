.pagerbox {
	background-color: #fff;
	width: 670px;
	padding: 0px 10px 20px 20px;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;	
	behavior: url(/PIE.htc);
	/*
	margin-top: 15px;
	margin-bottom: 15px;
	*/
	color: #4b4b4b;
}

.pager_status {
	float: left;
}

.pagerbox ul {
	padding-left: 150px;
	padding-top: 4px;
}

.pagerbox li {
	float: left;
	padding-right: 10px;
}

.pagerbox a {
	color: #0096d7;
	text-decoration: none;
	padding-left: 5px;
	padding-right: 5px;
}

.pagerbox a:hover {
	color: #003478;
	background-color: #ebebeb;
}

.pagerbox .pager-current {
	color: #929292;
	background-color: #f2f2f2;
	border: 1px solid #bbbbbb;
	padding-top: 3px !important;
	padding-bottom: 3px !important;
}

.pagerbox .active {
	color: #0096D7;
}

