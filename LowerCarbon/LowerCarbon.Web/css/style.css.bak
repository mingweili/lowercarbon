body {
	background-color: #e9e9e9;
	background-image: url(/images/bg.jpg);
	background-position: left top;
	background-repeat: repeat-x;
	font-family: Verdana, Arial, Tahoma, Helvetica Narrow, Lucida, Helvetica, Geneva, sans-serif;
	font-size: 62.5%;
}

h1,h3,h4 {
	font-family: 'carbontrustfont', 'Impact','Myriad Pro', Verdana, sans-serif;
	font-weight: normal;
	line-height: 1.8em;
}

h2 {
        font-family: 'carbontrustfont', 'Impact','Myriad Pro', Verdana, sans-serif;
        font-weight: normal;
}

/* TABLE */

table {
	width: 600px;
}

th, tr.icons, tr.source, tr.percentage {
	text-align: center;
	font-weight: normal;
}

th {
	color: #4b4b4b;
}

th span {
	display: block;
	padding-top: 20px;
	padding-bottom: 10px;
}

tr.percentage {
	font-family: 'carbontrustfont', 'Myriad Pro', Verdana, sans-serif;
	font-size: 1.8em;
}

td {
	padding-top: 20px;
}

caption {
	padding-bottom: 20px;
}

#submenu li.leaf, #submenu li.collapsed {
	list-style-image: none;
	list-style: none;
}

#slideshow {
	overflow:hidden;
	width: 960px;
	height: 330px;
}

#slideshowprev {
	position: absolute;
	z-index: 999;
	float: left;
	width: 80px;
	height: 322px;
	cursor: pointer;
	outline: 0;
	margin-left: 4px;
	margin-top: 3px;
}

#slideshownext {
	position: absolute;
	display: block;
	width: 80px;
	z-index: 999;	
	height: 322px;	
	cursor: pointer;	
	outline: 0;
	margin-left: 873px;
	margin-top: 3px;	
}

