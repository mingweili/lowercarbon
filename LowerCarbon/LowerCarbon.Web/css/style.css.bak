.skip {
	position: absolute; left: -999em;
}

a.skip:active,  a.skip:focus {
	top:0;
	left:0;
	color:#fff; 
	background: #20b7e5; 
	font-size:1.4em; 
	display:block; 
	padding:0.5em; 
	border:0.1em solid; 
	white-space:nowrap; 
	width:15em; 
	text-align:center; 
	font-weight:bold;
}

.clear {
	clear: both;
}

.clearright {
	clear: right;
}

.clear10 {
	clear:both;
	padding-top: 10px;
}

.clear16 {
	clear:both;
	padding-top: 16px;
}

.clear20 {
	clear:both;
	padding-top: 20px;
	width: 475px;
}

.clearsection {
	border-bottom: 1px dotted #999;
	padding-bottom: 20px;
	margin-bottom: 20px;
	clear: both;
	/*width: 650px;*/
	float: left;
	display:block;
	width: 500px;
}

.clearsection img {
	padding: 0 10px 5px 0;

}

.article img {
	padding: 0 10px 5px 0;

}
.clear img {
	padding: 0 10px 5px 0;

}

.clearsection a, .clearsection p, .clearsection h2 {
	/*float: left;
	display:block;*/
}

.clearsection a:hover, .clearsection a:active, .clearsection a:focus {
	color: #003478;
	text-decoration: underline;
}

.left {
	float: left;
	
}

.right {
	float: right;
}

.hidden {
	display: none;
}

.border-left {
	border-left: 1px dotted #999;
}

.border-bottom {
	border-bottom: 1px dotted #999;
}

.details {
	font-size: 0.8em;
	font-family: Verdana, Tahoma, sans-serif;
	color: #666;
	padding: 0px;
}

.wider {
	width: 580px !important;
}

.noborder {
	border: none !important;
}

.item-list ul li {
	margin: 0 0 0 8px;
}

.list_title {
	padding-left: 0px !important;
}

.profile h3 {
	border-bottom: none !important;
}

.form-item .description {
	font-size: 1em !important;
	padding-top: 0.5em;
}

.form-item label {
	padding-bottom: 0.5em;
}

#edit-pass-wrapper {
	padding-top: 1em;
}

.krumo-node li, .krumo-child {
	background: none !important;
}

.pager-first, .pager-previous, .pager-current, .pager-item, .pager-next, .pager-last {
	background-image: none !important;
	padding-left: 5px !important;
}

.flash img {
	float: left;
}

.flashdescription {
	float: left;
	width: 420px;
}

.flashdescription h2, .flashdescription p {
	width: 420px;
}


object {
	padding-left: 10px;
}

li.leaf, li.expanded {
	list-style-image: none;
	list-style-type: none;
}

.site-map-box {
	padding-left: 15px;
}

.views-field {
	padding: 20px;
}

.views-field a, .site-map-menu a, .site-map-menu .leaf, .site-map-menu .first, .site-map-menu .last {
	color: #0096D7;
	font-size: 1em !important;
	font-weight: normal;
}

.view-header h1 {
	padding-left: 15px;
}

.thelabel-active {
	color: #9fb21c;
}

.takeaction-active {
