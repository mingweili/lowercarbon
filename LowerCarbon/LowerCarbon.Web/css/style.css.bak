}

.backbutton {
	padding-left: 20px;
	font-weight: bold;
	background-image: url(/images/icon_back.gif);
	background-repeat: no-repeat;
	vertical-align: middle;	
	padding-bottom: 10px;
}

.flash-update {
	font-weight: bold;
}

.widercontent .clearsection, .widercontent .clear {
	width: 650px;
}

.widercontentflash p {
